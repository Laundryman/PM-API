{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    //"Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      //{
      //  "Name": "Console"
      //},
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-ixmat-.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "Properties": {
      "Application": "IXMat App",
      "Environment": "Development"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=XPSII;Initial Catalog=planMatrCore;Persist Security Info=True;User ID=planMatr;Password=Passw0rd10;TrustServerCertificate=True;",
    "PMContextConnection": "Data Source=XPSII;Initial Catalog=planMatrCore;Persist Security Info=True;User ID=planMatr;Password=Passw0rd10;TrustServerCertificate=True;"
  },
  "AllowedHosts": "*",
  "AzureAdB2C": {
    "Instance": "https://contoso.b2clogin.com",
    "Domain": "contoso.onmicrosoft.com",
    "ClientId": "<web-app-application-id>",
    "SignedOutCallbackPath": "/signout/<your-sign-up-in-policy>",
    "SignUpSignInPolicyId": "<your-sign-up-in-policy>"
  },
